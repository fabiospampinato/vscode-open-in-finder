{
    "name": "vscode-open-in-finder",
    "displayName": "Open in Finder",
    "description": "Adds a few commands for opening the current file or project in Finder.",
    "icon": "resources/logo-128x128.png",
    "version": "1.1.8",
    "license": "MIT",
    "main": "out/extension.js",
    "publisher": "fabiospampinato",
    "activationEvents": [
        "onCommand:openInFinder.open",
        "onCommand:openInFinder.openRoot"
    ],
    "contributes": {
        "commands": [
            {
                "command": "openInFinder.open",
                "title": "Open in Finder",
                "icon": {
                    "dark": "./icons/reveal.png",
                    "light": "./icons/reveal_light.png"
                }
            },
            {
                "command": "openInFinder.openRoot",
                "title": "Open Root in Finder"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "openInFinder.open",
                    "group": "navigation",
                    "when": "editorIsOpen && resourceScheme != 'untitled' && resourceScheme != 'walkThrough'"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "rm -rf out && webpack --mode production",
        "publish": "vsce publish && npm publish",
        "compile": "webpack --mode development",
        "compile:watch": "webpack --mode development --watch",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "bugs": {
        "url": "https://github.com/fabiospampinato/vscode-open-in-finder/issues"
    },
    "author": {
        "name": "Fabio Spampinato",
        "email": "spampinabio@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fabiospampinato/vscode-open-in-finder.git"
    },
    "engines": {
        "vscode": "^1.15.0"
    },
    "keywords": [
        "vscode",
        "vsc",
        "extension",
        "open in",
        "finder"
    ],
    "categories": [
        "Other"
    ],
    "dependencies": {
        "@types/node": "^10.12.8",
        "absolute": "0.0.1",
        "applescript": "^1.0.0",
        "lodash": "^4.17.4",
        "open": "0.0.5"
    },
    "devDependencies": {
        "ts-loader": "^5.2.1",
        "typescript": "~2.4.1",
        "vscode": "^1.1.4",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.2"
    }
}
